	background: linear-gradient(top, #252525 0%, #484848 100%); /* W3C */
	display: block;
	float: left;
	margin: 0 10px 0 0;
	color: #fff !important;
	font: 10px/12px "Times New Roman", Times, serif;
	letter-spacing: 3px;
	padding: 7px 17px;
	text-shadow: 0px 1px 0px #000000;
	text-transform: uppercase;
	filter: dropshadow(color=#000000, offx=0, offy=1);
	border: 1px solid #000;
	position: relative;
}
