}
div.navigationPopup ul li {
	float:left;
}
div#header div.navigationPopup ul li a {
	color:#999999;
	text-indent:0 !important;
	background:none !important;
	padding: 0;
	display:block;
	height:auto;
}
div#header ul#navigation li div.navigationPopup {
	padding:20px 0;
}
div#header ul#navigation li div.navigationPopup ul {
	border-bottom:1px solid #1e1e1e;
	margin-bottom:10px;
	padding:0 0 10px 20px;
}
div#header ul#navigation li div.navigationPopup ul ul {
	padding:0;
	border:0 none;
	margin:0;
}
div#header ul#navigation li div.navigationPopup ul li {
	width:133px;
}
div#header ul#navigation li div.navigationPopup ul li a {
	margin-bottom:2px;
	font-size:10px;
	color:#999999;
	cursor:default;
	text-transform:uppercase;
}
/*AF*/
div#header ul#navigation li div.navigationPopup ul li a.link {
	margin-bottom:2px;
	font-size:10px;
	color:#FFF;
	cursor:pointer;
	text-transform:uppercase;
}
div#header ul#navigation li div.navigationPopup ul li a.link:hover {
	color: #CCC;
	text-decoration: underline;	
}

div#header ul#navigation li div.navigationPopup ul li ul li a {
	color:#FFF;
	cursor:pointer;
}

div#header ul#navigation li div.navigationPopup ul li ul li a:hover {
	color: #CCC;
	text-decoration: underline;	
}

