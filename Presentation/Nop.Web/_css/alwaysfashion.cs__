	background: -webkit-linear-gradient(top, #252525 8%, #484848 80%, #484848 83%, #484848 98%) !important; /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #252525 8%, #484848 80%, #484848 83%, #484848 98%) !important; /* Opera11.10+ */
	background: -ms-linear-gradient(top, #252525 8%, #484848 80%, #484848 83%, #484848 98%) !important; /* IE10+ */
 	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#252525', endColorstr='#484848', GradientType=0 ) !important; /* IE6-9 */
	background: linear-gradient(top, #252525 8%, #484848 80%, #484848 83%, #484848 98%) !important; /* W3C */
	text-align:center;
	text-transform:uppercase;
	font-family:"Times New Roman", Times, serif;
	font-size:10px;
	color:#ffffff;
	letter-spacing:3px;
	display:block;
	width:170px;
	height:23px;
	border:1px solid #000;
	padding-top:3px;
	voice-family: "\"}\""; voice-family:inherit;
	height:20px;
	cursor:pointer;
	
}
