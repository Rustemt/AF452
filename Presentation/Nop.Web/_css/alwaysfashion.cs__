/*AF input button added*/
div#content input.button1, a.button1, div#giftToolBottom a.button1, div#loginLeft a.continue, div#loginRight a.register, ul.continueShopping li.checkOut a, div.navigationPopup a.button1, input.continue{
	background: #252525; /* Old browsers */
	background: -moz-linear-gradient(top, #252525 0%, #484848 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #252525), color-stop(100%, #484848)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #252525 0%, #484848 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #252525 0%, #484848 100%); /* Opera11.10+ */
	background: -ms-linear-gradient(top, #252525 0%, #484848 100%); /* IE10+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#252525', endColorstr='#484848', GradientType=0 ); /* IE6-9 */
