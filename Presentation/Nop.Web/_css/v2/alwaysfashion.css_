.pagination {
	margin-top: 20px;
}

.pagination.ngg {
	clear: both;
	padding-top: 10px;
	padding-bottom: 20px;
}

.pagination ul {
	display: table;
	margin: 0 auto;
}

.pagination ul li {
	display: inline-block;
	height: 12px;
	line-height: 12px;
	margin-right: 1px;
	padding-left: 1px;
	border-left: 1px solid #E5E5E5;
}

.pagination ul li:first-child {
	border-left: 0;
	padding-left: 0;
}

.pagination ul li a, .pagination ul li span, .ngg-browser-prev, .ngg-browser-next {
	display: block;
	color: #666;
	font: 10px/18px "Times New Roman";
	padding: 0 6px;
	margin-top: -3px;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	border-radius: 2px;
}

.pagination ul li span {
	background-color: #666;
	color: #FFF;
	line-height: 20px;
}

.pagination ul li strong {
	background-color: #666;
	color: #FFF;
}

.pagination ul li.next, .pagination ul li.prev {
	font-family: "Lucida Sans Unicode", "Lucida Sans";
}

.pagination ul li a:hover, .ngg-imagebrowser-nav a:hover {
	background-color: #e5e5e5;
}

.pagination ul li input[type="text"], input[type="password"], textarea, select { 
    outline: none;
}

.pagination ul li .currentPageInput {
        width: 22px;
        height: 20px;
        line-height: 20px;
        text-align: center;
        font-size: 1em;
        float: left;
		background-color: #666;
		color: #FFF;
        border: none;
        margin-top: -1px;
		margin-bottom: -3px;
        padding: 0;
		-moz-border-radius: 2px;
		-webkit-border-radius: 2px;
		border-radius: 2px;
    }
